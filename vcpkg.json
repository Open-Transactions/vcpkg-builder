{
    "$schema":
        "https://raw.githubusercontent.com/microsoft/vcpkg/master/scripts/vcpkg.schema.json",
    "name": "opentxs-dependency-builder",
    "version": "1",
    "dependencies": [
        {
            "name": "boost-algorithm",
            "default-features": false
        },
        {
            "name": "boost-asio",
            "default-features": false,
            "features": [
                "ssl"
            ]
        },
        {
            "name": "boost-beast",
            "default-features": false
        },
        {
            "name": "boost-circular-buffer",
            "default-features": false
        },
        {
            "name": "boost-container",
            "default-features": false
        },
        {
            "name": "boost-date-time",
            "default-features": false
        },
        {
            "name": "boost-dynamic-bitset",
            "default-features": false
        },
        {
            "name": "boost-endian",
            "default-features": false
        },
        {
            "name": "boost-interprocess",
            "default-features": false
        },
        {
            "name": "boost-iostreams",
            "default-features": false,
            "features": [
                "zlib"
            ]
        },
        {
            "name": "boost-json",
            "default-features": false
        },
        {
            "name": "boost-multiprecision",
            "default-features": false
        },
        {
            "name": "boost-program-options",
            "default-features": false
        },
        {
            "name": "boost-scope",
            "default-features": false
        },
        {
            "name": "boost-smart-ptr",
            "default-features": false
        },
        {
            "name": "boost-stacktrace",
            "default-features": false
        },
        {
            "name": "boost-stacktrace",
            "default-features": false,
            "features": [
                "backtrace"
            ],
            "platform": "!(windows | ios)"
        },
        {
            "name": "boost-stacktrace",
            "default-features": false,
            "features": [
                "windbg"
            ],
            "platform": "windows"
        },
        {
            "name": "boost-system",
            "default-features": false
        },
        {
            "name": "boost-thread",
            "default-features": false
        },
        {
            "name": "boost-type-index",
            "default-features": false
        },
        {
            "name": "boost-unordered",
            "default-features": false
        },
        {
            "name": "cmakerc",
            "default-features": false
        },
        {
            "name": "libguarded",
            "default-features": false
        },
        {
            "name": "libsodium",
            "version>=": "1.0.20#103"
        },
        {
            "name": "lmdb",
            "default-features": false
        },
        {
            "name": "openssl",
            "version>=": "3.2.0",
            "default-features": false
        },
        {
            "name": "otcommon",
            "version>=": "3.1.3",
            "default-features": false
        },
        {
            "name": "protobuf",
            "default-features": false
        },
        {
            "name": "protobuf",
            "host": true
        },
        {
            "name": "pthread",
            "platform": "windows",
            "default-features": false
        },
        {
            "name": "qtbase",
            "default-features": false,
            "features": [
                "gui",
                "widgets"
            ]
        },
        {
            "name": "qtdeclarative",
            "default-features": false
        },
        {
            "name": "secp256k1",
            "default-features": false
        },
        {
            "name": "simpleini",
            "default-features": false
        },
        {
            "name": "sqlite3",
            "default-features": false
        },
        {
            "name": "tbb",
            "default-features": false
        },
        {
            "name": "zeromq",
            "default-features": false,
            "features": [
                "curve",
                "sodium"
            ]
        },
        {
            "name": "zlib",
            "default-features": false
        }
    ]
}
